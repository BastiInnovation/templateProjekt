<mvc:View
	controllerName="ifm.bs.template.templateproject.controller.App"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:core="sap.ui.core"
	xmlns:grid="sap.ui.layout.cssgrid"
	xmlns:mvc="sap.ui.core.mvc">

	<App id="app">
	<Page id="page">
		<content> 
			<List
				headerText="Verein"
				items="{bundesligaPlacing>/}"> 
					<items>
				<StandardListItem
				
				

				title="{bundesligaPlacing>teamName}"
				/>
					</items>
				
				<ObjectListItem
					title = "{bundesligaPlacing>teamName}">
					<firstStatus>
						<ObjectStatus text="{
                        	    path: 'bundesligaPlacing>points',
                            	formatter : '.formatter.points'
                            	}"
								
						icon ="C:\Users\DEKNEBNI\Documents\templateProjekt\src\model\bild.png"/>
								
					</firstStatus>

				</ObjectListItem>
			</List>
		</content>
	</Page>
	</App>

</mvc:View>